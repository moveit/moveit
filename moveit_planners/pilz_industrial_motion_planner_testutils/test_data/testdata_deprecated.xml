            </group>
        </pos>

        <pos name="CIRCInterimPose">
            <group name="manipulator">
                <xyzQuat link_name="prbt_tcp">0.435 -0.0835 0.67 0.0 0.0 0.0 1.0</xyzQuat>
            </group>
        </pos>

        <pos name="CIRCGoalPose">
            <group name="manipulator">
